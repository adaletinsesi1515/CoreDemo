@using static CoreDemo.Controllers.TestEmployeeController
@model List<Class1>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table>
    <tr>
        <th>#</th>
        <th>Adı Soyadı</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>    
        @foreach (var item in Model)
       {
           <tr>
           <td>@item.ID</td>
           <td>@item.Name</td>
           <td><a href="#" class="btn btn-info">Sil</a></td>
           <td><a href="#" class="btn btn-info">Düzenle</a></td>
           </tr>
       }
    
</table>

